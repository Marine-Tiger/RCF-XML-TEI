   <define name="list">
      <element name="list">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(list) contains any sequence of items organized as a list. [3.8. Lists]</a:documentation>
         <group>
            <zeroOrMore>
               <choice>
                  <ref name="model.divTop"/>
                  <ref name="model.global"/>
                  <zeroOrMore>
                     <ref name="desc"/>
                  </zeroOrMore>
               </choice>
            </zeroOrMore>
            <choice>
               <oneOrMore>
                  <group>
                     <ref name="item"/>
                     <zeroOrMore>
                        <ref name="model.global"/>
                     </zeroOrMore>
                  </group>
               </oneOrMore>
               <group>
                  <optional>
                     <ref name="headLabel"/>
                  </optional>
                  <optional>
                     <ref name="headItem"/>
                  </optional>
