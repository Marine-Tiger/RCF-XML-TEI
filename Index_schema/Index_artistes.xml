                  <!-- feminin -->
                  <gender/>
               </person>
            </listPerson>

            <!-- TABLE AUTEURS Pour les auteurs -->
            <listPerson>
               <person xml:id="AUTE1">
                  <idno type="base_unifiee_auteurs"></idno>
                  <persName>
                     <!-- Nom -->
                     <reg></reg>
